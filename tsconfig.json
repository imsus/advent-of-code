{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "types": [
            "node"
        ],
        "resolveJsonModule": true
    }
}